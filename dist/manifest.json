{
  "manifest_version": 3,
  "name": "GPT Organizer",
  "version": "0.0.1",
  "description": "Organiza tus chats de ChatGPT en carpetas",
  "host_permissions": ["https://chatgpt.com/*"],
  "content_scripts": [
    {
      "matches": ["https://chatgpt.com/*"],
      "js": ["assets/content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
